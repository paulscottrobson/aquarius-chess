C199: CD65C0  			call 	CompileByte
C19C: C3F5C0  			jp 		_CEExecuteOrCompile 		; and go do it, perhaps.		
              	
              	; ***************************************************************************************
              	;
              	; 						Write dictionary word out working downwards
              	;
              	; ***************************************************************************************
              	
C19F:         	CompileWriteDictionary:
C19F: E5      			push 	hl
