C04A: 3ECD    			ld 		a,$CD 						; CD is Z80 "CALL" 
C04C: CD63C0  			call 	CompileByte
C04F: E1      			pop 	hl 							; get address to compile call to
C050: CD54C0  			call 	CompileWord
C053: C9      			ret 								; return to the previous level.
              	
