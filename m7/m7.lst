C1A5: 2222C0  			ld 		(DictionaryBase),hl
C1A8: E1      			pop 	hl
C1A9: C9      			ret
              	
              	; ***************************************************************************************
              	;
              	;					 Put in HL the ASCIIZ constant following
              	;
              	; ***************************************************************************************
              	
