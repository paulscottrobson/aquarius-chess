              	; ***************************************************************************************
              	; ***************************************************************************************
              	;
              	;		Name : 		find.asm
              	;		Author :	Paul Robson (paul@robsons.org.uk)
              	;		Date : 		16th October 2021
              	;		Purpose :	Search dictionary
              	;
              	; ***************************************************************************************
              	; ***************************************************************************************
              	
              	; ***************************************************************************************
              	;
              	; 		Search dictionary for word at HL, which is internal format (2+6 ended with 
              	;		(2+space or $00). On exit HL points to the type byte or $0000 if not found.
              	;
              	; ***************************************************************************************
              	
C165:         	SearchDictionary:
C165: C5      			push 	bc
C166: D5      			push 	de
C167: EB      			ex 		de,hl 						; put search string address in DE.
C168: 2A22C0  			ld 		hl,(DictionaryBase) 		; HL points to dictionary base.
              			;
              			; 		Compare entry at HL vs word at DE
              			;
C16B:         	_SearchLoop:
C16B: 44      			ld 		b,h 						; copy current being searched to BC
C16C: 4D      			ld 		c,l 								
C16D: 7E      			ld 		a,(hl) 						; get the type byte.
C16E: B7      			or 	 	a 							; if zero, then fail as we've reached dictionary end.
C16F: 2827    			jr 		z,_SDFail
              			;
C171: 23      			inc  	hl 							; point to the first character
C172: 23      			inc 	hl 							; e.g. skip type byte and call address.
C173: 23      			inc 	hl
C174: D5      			push 	de 							; save search string address on stack.
              			;
              			; 		Compare string at HL vs String at DE
              			;
C175:         	_SearchCompare:
C175: 1A      			ld 		a,(de) 						; calculate xor of two characters
C176: AE      			xor 	(hl) 						; as we're only interested in lower 6 bits for compare
C177: E63F    			and 	$3F 						; check lower 6 bits only.
C179: 2015    			jr 		nz,_SearchNext 				; different, go to next word.
C17B: 7E      			ld 		a,(hl) 						; get dictionary byte successfully matched.
C17C: 13      			inc 	de 							; advance to next.
C17D: 23      			inc 	hl 		
C17E: 87      			add 	a,a 						; is bit 7 in the last match, indicating the last character of the word in the dictionary ?
C17F: 30F4    			jr 		nc,_SearchCompare 			; no, keep comparing
              			;
              			; 		Found the end of the word in the dictionary, HL points to the byte following, DE the character following.
              			;	 	We need to check it's the end of the word in the search text.
              			;
C181: 1A      			ld 		a,(de) 						; get the next character in the search word.
C182: D1      			pop 	de 							; having got the character, restore DE to original value.
C183: B7      			or 		a 							; is it $00 or xx10000 (any colour space)
C184: 2806    			jr 		z,_SDSucceed 				; if zero, we've found the word, end of buffer
C186: E63F    			and 	$3F 
C188: FE20    			cp 		$20
C18A: 20DF    			jr 		nz,_SearchLoop 				; no it wasn't any Space so go round again. HL points to the next type byte already.
C18C:         	_SDSucceed: 
C18C: 60      			ld 		h,b 						; restore value saved in BC to HL, the type byte address of the found word.
C18D: 69      			ld 		l,c  						; at the start, e.g. the address of the type byte.
C18E: 180B    			jr 		_SDExit
              			;
              			; 		Names did not match. HL still points into the word.
              			;
C190:         	_SearchNext:
C190: 7E      			ld 		a,(hl) 						; get and bump in dictionary
C191: 23      			inc 	hl
C192: 87      			add 	a,a 						; looking for the bit 7 set indicating the end of the word
C193: 30FB    			jr 		nc,_SearchNext  			
C195: D1      			pop 	de 							; restore DE, the address of the input word
C196: 18D3    			jr 		_SearchLoop 				; and try the next dictionary word.
              	;
C198:         	_SDFail:									; not found, return zero
C198: 210000  			ld 		hl,$0000 					
C19B:         	_SDExit:
C19B: D1      			pop 	de
C19C: C1      			pop 	bc		
C19D: C9      			ret		
              	
              	
