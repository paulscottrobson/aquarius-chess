C055: E5      			push 	hl 							; save HL
C056: 2A1EC0  			ld 		hl,(CodeNextFree) 			; get code address
C059: 73      			ld 		(hl),e 						; write out
C05A: 23      			inc 	hl 
C05B: 72      			ld 		(hl),d
C05C: 23      			inc 	hl
C05D: 221EC0  			ld 		(CodeNextFree),hl 			; update code address
C060: E1      			pop 	hl 							; restore HL, DE
C061: EB      			ex 		de,hl 							
C062: C9      			ret
              	
